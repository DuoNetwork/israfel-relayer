{
	"name": "israfel-relayer",
	"version": "1.0.0",
	"private": true,
	"description": "Israfel Relayer",
	"author": "FinBook Pte Ltd",
	"scripts": {
		"test": "jest",
		"tslint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts{,x} --fix",
		"relayer": "ts-node ./src/index relayer",
		"addOrders": "ts-node ./src/client/addOrders",
		"cancelOrders": "ts-node ./src/client/cancelOrders",
		"pruneOrder": "ts-node ./src/index pruneOrder",
		"setAllowance": "ts-node ./src/index setAllowance",
		"orderWatcher": "ts-node ./src/index orderWatcher",
		"sequence": "ts-node ./src/index sequence",
		"orders": "ts-node ./src/index orders"
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"devDependencies": {
		"@types/ioredis": "^4.0.3",
		"@types/jest": "^23.3.1",
		"@types/ws": "^6.0.0",
		"jest": "^23.5.0",
		"pre-commit": "^1.2.2",
		"ts-jest": "^23.1.4",
		"tslint": "^5.11.0"
	},
	"dependencies": {
		"@0xproject/json-schemas": "^1.0.1",
		"@0xproject/types": "^1.0.1",
		"@0xproject/web3-wrapper": "^2.0.2",
		"0x.js": "^1.0.1",
		"aws-sdk": "^2.323.0",
		"ioredis": "^4.2.0",
		"moment": "^2.22.2",
		"ts-node": "^7.0.1",
		"typescript": "^3.0.1",
		"web3": "^1.0.0-beta.35",
		"ws": "^6.0.0"
	}
}
