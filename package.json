{
	"name": "duo-relayer",
	"version": "2.0.0",
	"private": true,
	"description": "Duo Relayer",
	"author": "FinBook Pte Ltd",
	"scripts": {
		"test": "jest",
		"tslint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts{,x} --fix",
		"server": "ts-node ./src/utils/server",
		"start": "cross-env ganache-cli --gasLimit 10000000 --noVMErrorsOnRPCResponse --db 0x_ganache_snapshot --noVMErrorsOnRPCResponse -p 8545 --networkId 50 -m \"concert load couple harbor equip island argue ramp clarify fence smart topic\"",
		"download_snapshot": "cross-env download --extract --strip 1 --out 0x_ganache_snapshot https://s3.amazonaws.com/testrpc-shapshots/965d6098294beb22292090c461151274ee6f9a26.zip",
		"relayer_ws": "ts-node ./src/client/index",
		"init_orderbook": "ts-node ./src/client/initializeOrderbook",
		"send_orders": "ts-node ./src/client/sendOrdersWithIntervals",
		"actions": "ts-node ./src/actions/index",
		"order_watcher": "ts-node ./src/orderWatcher"
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"config": {
		"mnemonic": "concert load couple harbor equip island argue ramp clarify fence smart topic",
		"snapshot_url": "https://s3.amazonaws.com/testrpc-shapshots/965d6098294beb22292090c461151274ee6f9a26.zip"
	},
	"devDependencies": {
		"@types/jest": "^23.3.1",
		"@types/request": "^2.47.1",
		"@types/ws": "^6.0.0",
		"download-cli": "^1.1.1",
		"firebase-tools": "^4.2.0",
		"jest": "^23.5.0",
		"ts-jest": "^23.1.4",
		"tslint": "^5.11.0",
		"0x.js": "1.0.1",
		"@0xproject/connect": "2.0.0",
		"@0xproject/subproviders": "^2.0.2",
		"@0xproject/web3-wrapper": "^2.0.2",
		"body-parser": "^1.18.3",
		"cli-table": "^0.3.1",
		"express": "^4.16.3"
	},
	"dependencies": {
		"0x.js": "^1.0.1",
		"@0xproject/connect": "^2.0.0",
		"@0xproject/json-schemas": "^1.0.1",
		"@0xproject/order-watcher": "^1.0.1",
		"@0xproject/types": "^1.0.1",
		"@0xproject/utils": "^1.0.8",
		"@0xproject/web3-wrapper": "^2.0.2",
		"@types/web3-provider-engine": "^14.0.0",
		"firebase-admin": "^6.0.0",
		"moment": "^2.22.2",
		"ts-node": "^7.0.1",
		"ws": "^6.0.0",
		"cross-env": "^5.1.4",
		"download-cli": "^1.0.5",
		"ganache-cli": "^6.1.6",
		"tslint": "5.11.0",
		"typescript": "3.0.1"
	}
}
